{
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    },
    {
      "source": "/workbox-*.js",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    }
  ]
}
